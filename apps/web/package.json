{
	"name": "@glfonline/web",
	"version": "0.0.0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"check": "pnpm biome check && pnpm run check:types",
		"check:format": "pnpm biome format",
		"check:lint": "pnpm biome lint",
		"check:types": "tsc --noEmit",
		"clean": "pnpm clean:outputs && rm -rf node_modules",
		"clean:outputs": "rm -rf .cache && rm -rf .turbo && rm -rf api && rm -rf public/build",
		"dev": "remix vite:dev",
		"fix": "pnpm biome check --write",
		"fix:format": "pnpm biome format --write",
		"fix:lint": "pnpm biome lint --write",
		"start": "NODE_OPTIONS='--import ./instrumentation.server.mjs' dotenv -e .env -- remix-serve ./build/server/index.js"
	},
	"dependencies": {
		"@algolia/client-search": "^5.5.3",
		"@glfonline/sanity-client": "workspace:^",
		"@glfonline/shopify-client": "workspace:^",
		"@headlessui/react": "^2.1.8",
		"@heroicons/react": "^2.1.5",
		"@portabletext/react": "^3.1.0",
		"@remix-run/css-bundle": "^2.12.1",
		"@remix-run/node": "^2.12.1",
		"@remix-run/react": "^2.12.1",
		"@sanity/asset-utils": "^1.3.0",
		"@sanity/image-url": "^1.0.2",
		"@sentry/remix": "^8.30.0",
		"@sentry/vite-plugin": "^2.22.4",
		"@shopify/hydrogen": "^2024.7.7",
		"@tanstack/query-core": "^5.56.2",
		"@tanstack/query-sync-storage-persister": "^5.56.2",
		"@tanstack/react-query": "^5.56.2",
		"@tanstack/react-query-devtools": "^5.56.2",
		"@tanstack/react-query-persist-client": "^5.56.2",
		"@unpic/react": "^0.1.14",
		"@vercel/analytics": "^1.3.1",
		"@vercel/node": "^3.2.15",
		"@vercel/remix": "^2.12.0",
		"@vercel/speed-insights": "^1.0.12",
		"algoliasearch": "^5.5.3",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"dedent": "^1.5.3",
		"emery": "^1.4.3",
		"is-ip": "^5.0.1",
		"isbot": "^5.1.17",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-intersection-observer": "^9.13.1",
		"react-turnstile": "^1.1.3",
		"react-zorm": "^0.9.0",
		"remeda": "^2.14.0",
		"remix-seo": "^0.1.0",
		"remix-utils": "^7.7.0",
		"tailwind-merge": "^2.5.2",
		"tiny-invariant": "^1.3.3",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@glfonline/tsconfig": "workspace:^",
		"@remix-run/dev": "^2.12.1",
		"@remix-run/serve": "^2.12.1",
		"@remix-run/server-runtime": "^2.12.1",
		"@sendgrid/mail": "^8.1.3",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/line-clamp": "^0.4.4",
