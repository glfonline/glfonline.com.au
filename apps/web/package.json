{
	"name": "@glfonline/web",
	"version": "0.0.0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "mkdir -p build/client/.vite && react-router build",
		"check:deps": "pnpm --dir ../.. exec knip --workspace apps/web --no-exit-code --dependencies",
		"check:exports": "pnpm --dir ../.. exec knip --workspace apps/web --no-exit-code --exports",
		"check:format": "pnpm biome format",
		"check:lint": "pnpm biome lint",
		"check:types": "tsc --noEmit",
		"clean": "pnpm clean:outputs && rm -rf node_modules",
		"clean:outputs": "rm -rf .cache && rm -rf .turbo && rm -rf api && rm -rf public/build",
		"dev": "react-router dev",
		"fix:deps": "pnpm --dir ../.. exec knip --workspace apps/web --fix --fix-type dependencies --no-exit-code",
		"fix:exports": "pnpm --dir ../.. exec knip --workspace apps/web --fix --fix-type exports --no-exit-code",
		"fix:format": "pnpm biome format --write",
		"fix:lint": "pnpm biome lint --write",
		"start": "NODE_OPTIONS='--import ./instrumentation.server.mjs' dotenv -e .env -- remix-serve \"$(find ./build/server -name 'index.js' -type f | head -1)\"",
		"test:all": "pnpm test:unit && pnpm test:browser",
		"test:browser": "vitest run --config=vitest.browser.config.ts",
		"test:coverage": "vitest run --coverage --config vitest.unit.config.ts",
		"test:ui": "vitest --ui",
		"test:unit": "vitest run --config vitest.unit.config.ts"
	},
	"dependencies": {
		"@algolia/client-search": "^5.47.0",
		"@glfonline/sanity-client": "workspace:^",
		"@glfonline/shopify-client": "workspace:^",
		"@headlessui/react": "^2.2.9",
		"@heroicons/react": "^2.2.0",
		"@portabletext/react": "^6.0.2",
		"@react-router/node": "^7.13.0",
		"@sanity/asset-utils": "^2.3.0",
		"@sanity/image-url": "^2.0.3",
		"@sentry/react-router": "^10.37.0",
		"@tanstack/query-async-storage-persister": "^5.90.22",
		"@tanstack/query-core": "^5.90.20",
		"@tanstack/react-form": "^1.28.0",
		"@tanstack/react-form-remix": "^1.28.0",
		"@tanstack/react-query": "^5.90.20",
		"@tanstack/react-query-devtools": "^5.91.2",
		"@tanstack/react-query-persist-client": "^5.90.22",
		"@unpic/react": "^1.0.2",
		"@vercel/node": "^5.5.28",
		"@vercel/react-router": "^1.2.5",
		"algoliasearch": "^5.47.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dedent": "^1.7.1",
		"emery": "^1.4.4",
		"is-ip": "^5.0.1",
		"isbot": "^5.1.34",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-intersection-observer": "^10.0.2",
		"react-router": "^7.13.0",
		"react-router-dom": "^7.13.0",
		"react-turnstile": "^1.1.4",
		"remeda": "^2.33.4",
		"remix-utils": "^9.0.0",
		"resend": "^6.9.1",
		"tailwind-merge": "^2.6.0",
		"tiny-invariant": "^1.3.3",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@glfonline/tsconfig": "workspace:^",
		"@react-router/dev": "^7.13.0",
		"@react-router/fs-routes": "^7.13.0",
		"@react-router/serve": "^7.13.0",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@types/node": "^24.10.9",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/browser-playwright": "^4.0.18",
		"autoprefixer": "^10.4.23",
		"dotenv-cli": "^11.0.0",
		"knip": "^5.82.1",
		"playwright": "^1.58.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.19",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-react": "^2.0.4"
	}
}
