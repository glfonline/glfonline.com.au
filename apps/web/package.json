{
	"name": "@glfonline/web",
	"version": "0.0.0",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "pnpm run build:css && pnpm run build:remix",
		"build:css": "tailwindcss -m -i ./styles/tailwind.css -o app/styles/tailwind.css",
		"build:remix": "remix build",
		"check": "pnpm run check:format && pnpm run check:lint && pnpm run check:types",
		"check:format": "pnpm prettier . --cache --check",
		"check:lint": "TIMING=1 eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"check:types": "tsc --noEmit",
		"dev": "concurrently \"pnpm run dev:css\" \"pnpm run dev:remix\"",
		"dev:css": "tailwindcss -w -i ./styles/tailwind.css -o app/styles/tailwind.css",
		"dev:remix": "remix dev",
		"lint": "TIMING=1 eslint \"./**/*.{js,ts,jsx,tsx}\""
	},
	"dependencies": {
		"@algolia/client-search": "^4.14.3",
		"@algolia/transporter": "^4.14.3",
		"@glfonline/sanity-client": "0.0.0",
		"@glfonline/shopify-client": "0.0.0",
		"@headlessui/react": "^1.7.11",
		"@heroicons/react": "^2.0.16",
		"@portabletext/react": "^2.0.1",
		"@remix-run/node": "^1.13.0",
		"@remix-run/react": "^1.13.0",
		"@remix-run/vercel": "^1.13.0",
		"@sanity/asset-utils": "^1.3.0",
		"@sanity/image-url": "^1.0.2",
		"@shopify/hydrogen": "^2023.1.5",
		"@tanstack/query-core": "^4.24.6",
		"@tanstack/query-sync-storage-persister": "^4.24.6",
		"@tanstack/react-query": "^4.24.6",
		"@tanstack/react-query-devtools": "^4.24.6",
		"@tanstack/react-query-persist-client": "^4.24.6",
		"@vercel/node": "^2.9.6",
		"algoliasearch": "^4.14.3",
		"class-variance-authority": "^0.4.0",
		"clsx": "^1.2.1",
		"dedent": "^0.7.0",
		"emery": "^1.4.1",
		"nodemailer": "^6.9.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-intersection-observer": "^9.4.2",
		"react-zorm": "^0.6.1",
		"remix-seo": "^0.1.0",
		"zod": "^3.20.6"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@glfonline/eslint-config": "0.0.0",
		"@glfonline/tsconfig": "0.0.0",
		"@remix-run/dev": "^1.13.0",
		"@remix-run/serve": "^1.13.0",
		"@remix-run/server-runtime": "^1.13.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.9",
		"@types/dedent": "^0.7.0",
		"@types/node": "18.14.1",
		"@types/nodemailer": "^6.4.7",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"concurrently": "^7.6.0",
		"dotenv-cli": "^7.0.0",
		"eslint": "^8.34.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-tailwindcss": "^0.2.3",
		"remix": "^1.13.0",
		"tailwindcss": "^3.2.7",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	}
}
