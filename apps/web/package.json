{
	"name": "@glfonline/web",
	"version": "0.0.0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"check": "pnpm biome check && pnpm run check:types",
		"check:format": "pnpm biome format",
		"check:lint": "pnpm biome lint",
		"check:types": "tsc --noEmit",
		"clean": "pnpm clean:outputs && rm -rf node_modules",
		"clean:outputs": "rm -rf .cache && rm -rf .turbo && rm -rf api && rm -rf public/build",
		"dev": "remix vite:dev",
		"fix": "pnpm biome check --write",
		"fix:format": "pnpm biome format --write",
		"fix:lint": "pnpm biome lint --write",
		"start": "NODE_OPTIONS='--import ./instrumentation.server.mjs' dotenv -e .env -- remix-serve \"$(find ./build/server -name 'index.js' -type f | head -1)\"",
		"test:all": "pnpm test:unit && pnpm test:browser",
		"test:browser": "vitest run --config vitest.browser.config.ts",
		"test:coverage": "vitest run --coverage --config vitest.unit.config.ts",
		"test:ui": "vitest --ui",
		"test:unit": "vitest run --config vitest.unit.config.ts"
	},
	"dependencies": {
		"@algolia/client-search": "^5.35.0",
		"@glfonline/sanity-client": "workspace:^",
		"@glfonline/shopify-client": "workspace:^",
		"@headlessui/react": "^2.2.7",
		"@heroicons/react": "^2.2.0",
		"@portabletext/react": "^3.2.1",
		"@remix-run/node": "^2.17.0",
		"@remix-run/react": "^2.17.0",
		"@sanity/asset-utils": "^2.2.1",
		"@sanity/image-url": "^1.1.0",
		"@sentry/remix": "^8.55.0",
		"@sentry/vite-plugin": "^3.6.1",
		"@tanstack/query-core": "^5.83.1",
		"@tanstack/query-sync-storage-persister": "^5.83.1",
		"@tanstack/react-form": "^1.15.2",
		"@tanstack/react-query": "^5.84.1",
		"@tanstack/react-query-devtools": "^5.84.1",
		"@tanstack/react-query-persist-client": "^5.84.1",
		"@unpic/react": "^0.1.15",
		"@vercel/analytics": "^1.5.0",
		"@vercel/node": "^5.3.10",
		"@vercel/remix": "^2.16.7",
		"algoliasearch": "^5.35.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dedent": "^1.6.0",
		"emery": "^1.4.4",
		"is-ip": "^5.0.1",
		"isbot": "^5.1.29",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-intersection-observer": "^9.16.0",
		"react-turnstile": "^1.1.4",
		"remeda": "^2.27.2",
		"remix-seo": "^0.1.0",
		"remix-utils": "^8.8.0",
		"resend": "^4.7.0",
		"tailwind-merge": "^2.6.0",
		"tiny-invariant": "^1.3.3",
		"zod": "^4.0.14"
	},
	"devDependencies": {
		"@glfonline/tsconfig": "workspace:^",
		"@remix-run/dev": "^2.17.0",
		"@remix-run/serve": "^2.17.0",
		"@remix-run/server-runtime": "^2.17.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tailwindcss/typography": "^0.5.16",
		"@types/dedent": "^0.7.2",
		"@types/node": "^22.17.0",
		"@types/react": "^18.3.23",
		"@types/react-dom": "^18.3.7",
		"@vitejs/plugin-react": "^4.7.0",
		"@vitest/browser": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"dotenv-cli": "^8.0.0",
		"playwright": "^1.54.2",
		"postcss": "^8.5.6",
		"remix": "^2.17.0",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.2",
		"vite": "^7.1.5",
		"vitest": "^3.2.4",
		"vitest-browser-react": "^1.0.1"
	}
}
