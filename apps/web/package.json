		"autoprefixer": "^10.4.20",
		"dotenv-cli": "^7.4.2",
		"postcss": "^8.4.47",
		"remix": "^2.12.1",
		"tailwindcss": "^3.4.12",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.6.2",
		"vite": "^5.4.7"
