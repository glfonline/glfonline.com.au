{
	"name": "@glfonline/web",
	"version": "0.0.0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"check": "pnpm biome check && pnpm run check:types",
		"check:format": "pnpm biome format",
		"check:lint": "pnpm biome lint",
		"check:types": "tsc --noEmit",
		"clean": "pnpm clean:outputs && rm -rf node_modules",
		"clean:outputs": "rm -rf .cache && rm -rf .turbo && rm -rf api && rm -rf public/build",
		"dev": "react-router dev",
		"fix": "pnpm biome check --write",
		"fix:format": "pnpm biome format --write",
		"fix:lint": "pnpm biome lint --write",
		"start": "NODE_OPTIONS='--import ./instrumentation.server.mjs' dotenv -e .env -- remix-serve \"$(find ./build/server -name 'index.js' -type f | head -1)\"",
		"test:all": "pnpm test:unit && pnpm test:browser",
		"test:browser": "vitest run --config vitest.browser.config.ts",
		"test:coverage": "vitest run --coverage --config vitest.unit.config.ts",
		"test:ui": "vitest --ui",
		"test:unit": "vitest run --config vitest.unit.config.ts"
	},
	"dependencies": {
		"@algolia/client-search": "^5.41.0",
		"@glfonline/sanity-client": "workspace:^",
		"@glfonline/shopify-client": "workspace:^",
		"@headlessui/react": "^2.2.9",
		"@heroicons/react": "^2.2.0",
		"@portabletext/react": "^3.2.4",
		"@react-router/node": "^7.9.5",
		"@sanity/asset-utils": "^2.3.0",
		"@sanity/image-url": "^1.2.0",
		"@sentry/remix": "^10.22.0",
		"@sentry/vite-plugin": "^4.6.0",
		"@tanstack/query-core": "^5.90.5",
		"@tanstack/query-sync-storage-persister": "^5.90.7",
		"@tanstack/react-form": "^1.23.8",
		"@tanstack/react-query": "^5.90.5",
		"@tanstack/react-query-devtools": "^5.90.2",
		"@tanstack/react-query-persist-client": "^5.90.7",
		"@unpic/react": "^0.1.15",
		"@vercel/analytics": "^1.5.0",
		"@vercel/node": "^5.5.2",
		"@vercel/react-router": "^1.2.3",
		"algoliasearch": "^5.41.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dedent": "^1.7.0",
		"emery": "^1.4.4",
		"is-ip": "^5.0.1",
		"isbot": "^5.1.31",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-intersection-observer": "^10.0.0",
		"react-router": "^7.9.5",
		"react-router-dom": "^7.9.5",
		"react-turnstile": "^1.1.4",
		"remeda": "^2.32.0",
		"remix-seo": "^0.1.0",
		"remix-utils": "^9.0.0",
		"resend": "^6.3.0",
		"tailwind-merge": "^2.6.0",
		"tiny-invariant": "^1.3.3",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@glfonline/tsconfig": "workspace:^",
		"@react-router/dev": "^7.9.5",
		"@react-router/fs-routes": "^7.9.5",
		"@react-router/serve": "^7.9.5",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tailwindcss/typography": "^0.5.19",
		"@types/dedent": "^0.7.2",
		"@types/node": "^22.18.13",
		"@types/react": "^18.3.26",
		"@types/react-dom": "^18.3.7",
		"@vitejs/plugin-react": "^5.1.0",
		"@vitest/browser": "^4.0.5",
		"autoprefixer": "^10.4.21",
		"dotenv-cli": "^11.0.0",
		"playwright": "^1.56.1",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.18",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vitest": "^4.0.5",
		"vitest-browser-react": "^2.0.2"
	}
}
