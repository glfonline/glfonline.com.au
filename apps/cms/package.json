{
	"name": "@glfonline/cms",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "sanity build",
		"check": "pnpm run /^check:/",
		"check:format": "pnpm biome format",
		"check:lint": "pnpm biome lint",
		"check:types": "tsc --noEmit",
