{
	"name": "@glfonline/cms",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "sanity build",
		"check": "pnpm run check:format && pnpm run check:lint && pnpm run check:types",
		"check:format": "pnpm prettier . --cache --check",
		"check:lint": "TIMING=1 eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"check:types": "tsc --noEmit",
		"deploy": "sanity deploy",
		"deploy-graphql": "sanity graphql deploy",
		"dev": "sanity dev",
		"lint": "TIMING=1 eslint \"./**/*.ts*\"",
		"start": "sanity start"
	},
	"dependencies": {
		"@sanity/vision": "^3.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-is": "^18.2.0",
		"sanity": "^3.0.0",
		"styled-components": "^5.2.0"
	},
	"devDependencies": {
		"@glfonline/eslint-config": "0.0.0",
		"@glfonline/tsconfig": "0.0.0",
		"@sanity/eslint-config-studio": "^2.0.1",
		"eslint": "^8.30.0",
		"prettier": "^2.8.1",
		"typescript": "^4.9.4"
	}
}
