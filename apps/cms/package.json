		"deploy:studio": "sanity deploy",
		"dev": "sanity dev",
		"start": "sanity start"
	},
	"dependencies": {
		"@sanity/vision": "^3.56.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"sanity": "^3.56.0",
		"styled-components": "^6.1.13"
	},
	"devDependencies": {
		"@glfonline/tsconfig": "workspace:^",
		"typescript": "^5.5.4"
	}
}
