{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Remix",
	"extends": "./base.json",
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowJs": true,
		"jsx": "react-jsx",
		"noEmit": true
	},
	"exclude": ["node_modules"],
	"include": ["remix.env.d.ts", "**/*.ts", "**/*.tsx"]
}
