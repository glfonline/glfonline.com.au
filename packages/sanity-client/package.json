{
	"name": "@glfonline/sanity-client",
	"version": "0.0.0",
	"private": true,
	"license": "MIT",
	"main": "src/index.ts",
	"types": "src/index.ts",
	"scripts": {
		"check:types": "tsc --noEmit",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
		"dev": "ts-gql watch",
		"generate": "graphql-codegen --config sanity-codegen.json",
		"postinstall": "ts-gql build",
		"lint": "TIMING=1 eslint \"src/**/*.ts*\""
	},
	"dependencies": {
		"@ts-gql/compiler": "^0.16.1",
		"@ts-gql/fetch": "^0.1.2",
		"@ts-gql/tag": "^0.7.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@glfonline/eslint-config": "0.0.0",
		"@glfonline/tsconfig": "0.0.0",
		"@graphql-codegen/cli": "^2.16.1",
		"@graphql-codegen/schema-ast": "^2.6.0",
		"@types/node": "18.11.13",
		"eslint": "^8.30.0",
		"graphql": "^16.6.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4"
	},
	"ts-gql": {
		"addTypenames": false,
		"mode": "no-transform",
		"schema": "./sanity-schema.graphql"
	}
}
